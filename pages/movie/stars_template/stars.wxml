<template name="starsTemplate">
  <view class="stars_container">
    <view class="stars">
      <block wx:for="{{stars}}" wx:key="*these" wx:for-item="i">
        <image wx:if="{{i}}" src="/images/icon/star.png"></image>
        <image wx:else src="/images/icon/none-star.png"></image>
      </block>
    </view>
    <text>{{average}}</text>
  </view>
</template>